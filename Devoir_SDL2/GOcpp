#include "GO.h"


//Constructeur
GO::GO()
{
	this->life = 0;
	this->invincibleTimer = 0;
	this->x = 0;
	this->y = 0;
	this->checkpointActif = 0;
	this->respawnX = 0;
	this->respawnY = 0;
	this->frameNumber = 0;
	this->frameTimer = 0;
	this->frameMax = 0;
	this->etat = 0;
	this->direction = 0;
	this->onGround = 0;
	this->timerMort = 0;
	this->dirX = 0;
	this->dirY = 0;
	this->saveX = 0;
	this->saveY = 0;
	this->jump = 0;
	this->beginX = 0;
	this->beginY = 0;
	this->type = 0;
	this->player_dessus = 0;
}

//Accesseurs et mutateurs
int GO::getLife()const {
	return this->life;
 };

void GO::setLife(int value) { 
	this->life = value; 
};

int GO::getInvincibleTimer()const { 
	return this->invincibleTimer; 
};

void GO::setInvincibleTimer(int value) { 
	this->invincibleTimer = value;
};

int GO::getX()const { 
	return this->x; 
};

void GO::setX(int value) { 
	this->x = value; 
};

int GO::getY()const { 
	return this->y; 
};
	
void GO::setY(int value) {
	this->y = value; 
};

int GO::getH()const { 
	return this->h; 
};

void GO::setH(int value) { 
	this->h = value; 
};

int GO::getW()const { 
	return this->w; 
};

void GO::setW(int value) { 
	this->w = value; 
};

int GO::getCheckpointActif()const { 
	return this->checkpointActif; 
};

void GO::setCheckpointActif(int value) { 
	this->checkpointActif = value;
};

int GO::getRespawnX()const { 
	return this->respawnX; 
};
	
void GO::setRespawnX(int value) { 
	this->respawnX = value; 
};

int GO::getRespawnY()const { 
	return this->respawnY;
};

void GO::setRespawnY(int value) {
	this->respawnY = value; 
};

int GO::getFrameNumber()const { 
	return this->frameNumber;
};

void GO::setFrameNumber(int value) { 
	this->frameNumber = value; 
};

int GO::getFrameTimer()const { 
	return this->frameTimer;
};

void GO::setFrameTimer(int value) { 
	this->frameTimer = value; 
};

int GO::getFrameMax()const { 
	return this->frameMax; 
};

void GO::setFrameMax(int value) {
	this->frameMax = value; 
};

int GO::getEtat()const { 
	return this->etat; 
};

void GO::setEtat(int value) { 
	this->etat = value; 
};

int GO::getDirection()const { 
	return this->direction; 
};

void GO::setDirection(int value) { 
	this->direction = value; 
};

int GO::getOnGround()const { 
	return this->onGround; 
};

void GO::setOnGround(int value) { 
	this->onGround = value; 
};

int GO::getTimerMort()const { 
	return this->timerMort; 
};

void GO::setTimerMort(int value) { 
	this->timerMort = value; 
};

float GO::getDirX()const { 
	return this->dirX; 
};

void GO::setDirX(float value) { 
	this->dirX = value; 
};

float GO::getDirY()const { 
	return this->dirY; 
