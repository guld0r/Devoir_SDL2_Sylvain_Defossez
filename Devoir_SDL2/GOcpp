void GO::setSaveX(int value) { 
	this->saveX = value; 
};

int GO::getSaveY()const { 
	return this->saveY; 
};

void GO::setSaveY(int value) {
	this->saveY = value; 
};

int GO::getJump()const { 
	return this->jump; 
};

void GO::setJump(int value) { 
	this->jump = value; 
};

int GO::getBeginX()const { 
	return this->beginX; 
};

void GO::setBeginX(int value) { 
	this->beginX = value; 
};

int GO::getBeginY()const { 
	return this->beginY; 
};

void GO::setBeginY(int value) { 
	this->beginY = value; 
};

int GO::getType()const { 
	return this->type; 
};

void GO::setType(int value) { 
	this->type = value; 
};

int GO::getPlayer_Dessus()const { 
	return this->player_dessus;
};

void GO::setPlayer_Dessus(int value) { 
